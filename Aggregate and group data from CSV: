import pandas as pd

df = pd.read_csv("data.csv")

print("Original Data:")
print(df)

grouped = df.groupby('Category').agg({
'Sales': 'sum',
'Quantity': 'mean',
'Profit': 'sum'
})

print("\nAggregated Data:")
print(grouped)

grouped.to_csv("aggregated_data.csv")
print("\nAggregated data has been saved to 'aggregated_data.csv'")

